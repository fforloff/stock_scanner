<%= simple_form_for(@index) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :ticker %>
    <%= f.input :name %>
    <%= f.input :exchange %>
    <%= f.input :roar %>
    <%= f.input :_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
